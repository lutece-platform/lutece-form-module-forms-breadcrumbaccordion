<?xml version="1.0" encoding="UTF-8"?><plug-in>
    <name>forms-breadcrumbaccordion</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.5</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>module.forms.breadcrumbaccordion.module.description</description>
    <provider>module.forms.breadcrumbaccordion.module.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2018 Mairie de Paris</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>6.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <css-stylesheets>
        <css-stylesheets-scope>portal</css-stylesheets-scope>
        <css-stylesheet>forms/modules/breadcrumbaccordion/breadcrumbaccordion.css</css-stylesheet>
    </css-stylesheets>

    <!-- Admin features parameters -->
    <admin-features>
    	<admin-feature>
            <feature-id>BREADCRUMBACCORDION_MANAGEMENT</feature-id>
            <feature-title>module.forms.breadcrumbaccordion.adminFeature.manageBreadcrumbAccordion.name</feature-title>
            <feature-description>module.forms.breadcrumbaccordion.adminFeature.manageBreadcrumbAccordion.description</feature-description>
            <feature-level>1</feature-level>
            <feature-url>jsp/admin/plugins/forms/modules/breadcrumbaccordion/ManageBreadcrumbAccordion.jsp</feature-url>
            <feature-icon-url/>
      </admin-feature>
    </admin-features>    

    <!-- XPages parameters -->
    <applications>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
   </portlets>
</plug-in>